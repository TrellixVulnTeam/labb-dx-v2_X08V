      var TextComponent = /*#__PURE__*/function () {
        function TextComponent(angularPConnect, utils) {
          _classCallCheck(this, TextComponent);

          this.angularPConnect = angularPConnect;
          this.utils = utils;
          this.label$ = "";
          this.value$ = "";
          this.bRequired$ = false;
          this.bReadonly$ = false;
          this.bDisabled$ = false;
          this.bVisible$ = true;
          this.componentReference = "";
          this.format$ = "text";
          this.formattedUrl$ = ""; // Used with AngularPConnect

          this.angularPConnectData = {};
        }

        _createClass(TextComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            // First thing in initialization is registering and subscribing to the AngularPConnect service
            this.angularPConnectData = this.angularPConnect.registerAndSubscribeComponent(this, this.onStateChange); // Then, continue on with other initialization
            // call updateSelf when initializing

            this.checkAndUpdate();
          }
        }, {
          key: "ngOnDestroy",
          value: function ngOnDestroy() {
            if (this.angularPConnectData.unsubscribeFn) {
              this.angularPConnectData.unsubscribeFn();
            }
          }
        }, {
          key: "checkAndUpdate",
          value: function checkAndUpdate() {
            // Should always check the bridge to see if the component should
            // update itself (re-render)
            var bUpdateSelf = this.angularPConnect.shouldComponentUpdate(this); // ONLY call updateSelf when the component should update

            if (bUpdateSelf) {
              this.updateSelf();
            }
          } // updateSelf

        }, {
          key: "updateSelf",
          value: function updateSelf() {
            // moved this from ngOnInit() and call this from there instead...
            this.configProps$ = this.pConn$.resolveConfigProps(this.pConn$.getConfigProps());

            if (this.configProps$["value"] != undefined) {
              this.value$ = this.configProps$["value"];
            }

            this.label$ = this.configProps$["label"]; // TDB - get formats

            switch (this.formatAs$) {
              case "text":
                this.formattedValue$ = this.value$;
                break;

              case "date":
                this.formattedValue$ = this.generateDate(this.value$);
                break;

              case "date-time":
                this.formattedValue$ = this.generateDateTime(this.value$);
                break;

              case "time":
                if (this.value$) {
                  var timeParts = this.value$.split(":");
                  this.formattedValue$ = "".concat(timeParts[0], ":").concat(timeParts[1]);
                } else {
                  this.formattedValue$ = "";
                }

                break;

              case "url":
                this.formattedUrl$ = this.generateUrl(this.value$);
                this.formattedValue$ = this.value$;
                break;
            }
          } // Callback passed when subscribing to store change

        }, {
          key: "onStateChange",
          value: function onStateChange() {
            this.checkAndUpdate();
          }
        }, {
          key: "generateUrl",
          value: function generateUrl(sVal) {
            if (sVal.indexOf("https://") == 0 || sVal.indexOf("http://") == 0) {} else {
              // assume no http
              sVal = "http://" + sVal;
            }

            return sVal;
          }
        }, {
          key: "generateDate",
          value: function generateDate(sVal) {
            if (!sVal) return ""; // const value = new Intl.DateTimeFormat('default', {
            //   year: 'numeric',
            //   month: 'numeric',
            //   day: 'numeric'
            // }).format(new Date(sVal + "T00:00"));
            // sVal = moment(sVal, "YYYYMMDD").format("MM/DD/YYYY");

            return this.utils.generateDate(sVal, "Date-Long-Custom-YYYY");
          }
        }, {
          key: "generateDateTime",
          value: function generateDateTime(sVal) {
            if (!sVal) return "";
            if (sVal.length === 10) return this.generateDate(sVal);
            var value = sVal.substring(0, sVal.length - 1); // value = new Intl.DateTimeFormat('default', {
            //   year: 'numeric',
            //   month: 'numeric',
            //   day: 'numeric',
            //   hour: 'numeric',
            //   minute: 'numeric',
            //   second: 'numeric',
            //   hour12: true,
            // }).format(new Date(value))
            // sVal = moment(sVal, "YYYYMMDD").format("MM/DD/YYYY, hh:mm a");

            return this.utils.generateDateTime(value, "DateTime-Long-YYYY-Custom");
          }
        }]);

        return TextComponent;
      }();

