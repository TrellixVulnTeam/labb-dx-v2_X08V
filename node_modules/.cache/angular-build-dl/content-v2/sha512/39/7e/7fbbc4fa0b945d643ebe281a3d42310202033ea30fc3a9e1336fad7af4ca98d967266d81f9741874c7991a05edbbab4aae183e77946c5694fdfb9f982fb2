        }, {
          key: "getBuildName",
          value: function getBuildName() {
            // let { getPConnect, name } = this.pConn$.props;
            var context = this.pConn$.getContextName();
            var viewContainerName = this.pConn$.getContainerName();
            if (!viewContainerName) viewContainerName = "";
            return "".concat(context.toUpperCase(), "/").concat(viewContainerName.toUpperCase());
