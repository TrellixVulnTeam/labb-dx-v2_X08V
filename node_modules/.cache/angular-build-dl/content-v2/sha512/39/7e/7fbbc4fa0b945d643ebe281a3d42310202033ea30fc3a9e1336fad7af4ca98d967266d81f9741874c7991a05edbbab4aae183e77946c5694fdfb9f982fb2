      __webpack_exports__["default"] = ".psdk-nav-divider {\n  border-bottom: 0.0625rem solid #414141;\n  width: 100%;\n}\n\n.psdk-nav-header {\n  display: flex;\n  padding-top: 0.625rem;\n}\n\n.psdk-nav-logo {\n  width: 3.75rem;\n  padding: 0.625rem;\n}\n\n.psdk-nav-svg-icon {\n  filter: invert(100%) sepia(99%) saturate(0%) hue-rotate(144deg) brightness(109%) contrast(101%);\n  width: 1.6rem;\n  padding-right: 0.625rem;\n}\n\n.psdk-nav-portal-name {\n  font-size: 0.875rem;\n}\n\n.psdk-nav-portal-app {\n  font-size: 1rem;\n}\n\n.psdk-appshell-nav {\n  z-index: 3;\n  position: fixed;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  background: #262626;\n  width: 5rem;\n  height: 100%;\n  color: #d9d9d9;\n  overflow: hidden;\n  white-space: nowrap;\n  transition: width 0.3s cubic-bezier(0.4, 0.6, 0.1, 1);\n  will-change: width;\n}\n\n.psdk-appshell-nav:hover {\n  width: 15.625rem;\n}\n\n.psdk-appshell-topnav {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  background: #262626;\n  width: 5rem;\n  height: 15%;\n  color: #d9d9d9;\n  overflow: hidden;\n  white-space: nowrap;\n  transition: width 0.3s cubic-bezier(0.4, 0.6, 0.1, 1);\n  will-change: width;\n}\n\n.psdk-appshell-middlenav {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  background: #262626;\n  width: 5rem;\n  height: 50%;\n  color: #d9d9d9;\n  overflow: hidden;\n  white-space: nowrap;\n  transition: width 0.3s cubic-bezier(0.4, 0.6, 0.1, 1);\n  will-change: width;\n}\n\n.psdk-nav-ul-middle {\n  display: block;\n  list-style-type: none;\n  margin: 0rem;\n  -webkit-padding-start: 30px;\n          padding-inline-start: 30px;\n}\n\n.psdk-nav-li-middle {\n  box-sizing: border-box;\n  text-align: left;\n}\n\n.psdk-icon {\n  padding: 0rem 0.125rem;\n  min-width: unset;\n}\n\n.psdk-nav-button-span {\n  padding: 1rem;\n}\n\n.psdk-nav-oper-avatar {\n  margin: 0rem;\n  padding: 0rem;\n  min-width: 2.5rem;\n  min-height: 2