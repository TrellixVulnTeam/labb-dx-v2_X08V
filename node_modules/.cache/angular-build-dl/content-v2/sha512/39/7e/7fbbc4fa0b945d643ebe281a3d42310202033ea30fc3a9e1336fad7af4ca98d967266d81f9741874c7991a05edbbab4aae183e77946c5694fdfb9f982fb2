      var CancelAlertComponent = /*#__PURE__*/function () {
        function CancelAlertComponent(erService, psService) {
          _classCallCheck(this, CancelAlertComponent);

          this.erService = erService;
          this.psService = psService;
          this.onAlertState$ = new _angular_core__WEBPACK_IMPORTED_MODULE_3__["EventEmitter"]();
        }

        _createClass(CancelAlertComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            if (!this.PCore$) {
              this.PCore$ = window.PCore;
            }
          }
        }, {
          key: "ngOnChanges",
          value: function ngOnChanges(changes) {
            if (this.bShowAlert$) {
              this.psService.sendMessage(false);
              var contextName = this.pConn$.getContextName();
              var caseInfo = this.pConn$.getCaseInfo();
              var caseName = caseInfo.getName();
              var ID = caseInfo.getID();
              this.itemKey = contextName;
              this.heading$ = "Delete " + caseName + " (" + ID + ")";
              this.body1$ = "Are you sure you want to delete " + caseName + " (" + ID + ")?";
              this.body2$ = "Alternatively, you can continue working or save your work for later."; //this.onAlertState$.emit(true);
            }
          }
        }, {
          key: "ngOnDestroy",
          value: function ngOnDestroy() {}
        }, {
          key: "dismissAlert",
          value: function dismissAlert() {
            this.bShowAlert$ = false;
            this.onAlertState$.emit(false);
          }
        }, {
          key: "dismissAlertOnly",
          value: function dismissAlertOnly() {
            this.bShowAlert$ = false;
            this.onAlertState$.emit(true);
          }
        }, {
          key: "sendMessage",
          value: function sendMessage(sMessage) {
            //this.snackBarRef = this.snackBar.open(sMessage,"Ok", { duration: 3000});
            //this.erService.sendMessage("show", sMessage);
            alert(sMessage);
          }
        }, {
          key: "buttonClick",
          value: function buttonClick(sAction) {
            var _this29 = this;

            var dispatchInfo = {
              context: this.itemKey,
              semanticURL: ""
            };
            var actionsAPI = this.pConn$.getActionsApi();

            switch (sAction) {
              case "save":
                this.psService.sendMessage(true);
                var savePromise = actionsAPI.saveAndClose(this.itemKey);
                savePromise.then(function () {
                  _this29.psService.sendMessage(false);

                  _this29.dismissAlert();

                  _this29.PCore$.getPubSubUtils().publish(_this29.PCore$.getConstants().PUB_SUB_EVENTS.CASE_EVENTS.CASE_CREATED);
                })["catch"](function () {
                  _this29.psService.sendMessage(false);

                  _this29.sendMessage("Save failed");
                });
                break;

              case "continue":
                this.dismissAlertOnly();
                break;

              case "delete":
                this.psService.sendMessage(true);
                var deletePromise = actionsAPI.deleteCaseInCreateStage(this.itemKey);
                deletePromise.then(function () {
                  _this29.psService.sendMessage(false);

                  _this29.dismissAlert();

                  _this29.PCore$.getPubSubUtils().publish(_this29.PCore$.getConstants().PUB_SUB_EVENTS.EVENT_CANCEL);
                })["catch"](function () {
                  _this29.psService.sendMessage(false);

                  _this29.sendMessage("Delete failed.");
                });
                break;
            }
          }
        }]);

        return CancelAlertComponent;
      }();

      CancelAlertComponent.ctorParameters = function () {
        return [{
