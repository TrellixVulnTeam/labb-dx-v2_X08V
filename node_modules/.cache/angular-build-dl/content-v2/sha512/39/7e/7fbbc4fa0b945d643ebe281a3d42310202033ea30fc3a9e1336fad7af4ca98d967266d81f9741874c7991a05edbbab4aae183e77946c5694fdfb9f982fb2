    /***/
    "a8gA":
    /*!********************************************************!*\
      !*** ./src/app/_components/feed-container/feed-api.js ***!
      \********************************************************/

    /*! exports provided: default */

    /***/
    function a8gA(module, __webpack_exports__, __webpack_require__) {
      "use strict";

      __webpack_require__.r(__webpack_exports__);
      /* eslint-disable sonarjs/cognitive-complexity */


      var FeedApi = function FeedApi(pConnect) {
        var _PCore$getRestClient = PCore.getRestClient(),
            invokeRestApi = _PCore$getRestClient.invokeRestApi,
            getCancelTokenSource = _PCore$getRestClient.getCancelTokenSource,
            isRequestCanceled = _PCore$getRestClient.isRequestCanceled;

        var appName = window.PCore.getEnvironmentInfo().getApplicationName();
        var mentionsTagsCancelTokenSource = [];

        var getPulseContext = function getPulseContext(pulseContext) {
          if (pulseContext && pulseContext.indexOf("DATA-PORTAL") !== -1) {
            pulseContext = "DATA-PORTAL $".concat(appName);
          }

          return pulseContext;
        };

        var fetchMessages = function fetchMessages(pulseContext, feedID, feedClass, feedFilters, fetchMessagesCancelTokenSource) {
          pulseContext = getPulseContext(pulseContext);
          var filterBy = "";

          if (feedFilters) {
            feedFilters.forEach(function (feedFilter) {
              if (feedFilter.on) filterBy = "".concat(filterBy).concat(feedFilter.id, ",");
            });
            if (filterBy === "") filterBy = "ClearFilters";
          }

          var queryPayload = {
            filterForContext: pulseContext,
            feedID: feedID,
            feedClass: feedClass,
            filterBy: filterBy
          };

          for (var i = 0; i < fetchMessagesCancelTokenSource.length; i += 1) {
            fetchMessagesCancelTokenSource[i].cancel();
          }

          var newCancelTokenSource = getCancelTokenSource();
          fetchMessagesCancelTokenSource.push(newCancelTokenSource);
          return invokeRestApi("getFeedMessages", {
            queryPayload: queryPayload,
            cancelTokenSource: newCancelTokenSource
          }, pConnect.getContextName()).then(function (response) {
            if (response.status === 200 && response.data) {
              fetchMessagesCancelTokenSource.pop();
              var respMessageIDs = [];
              var respMessages = {};
              var feedViewResponse = response.data;

              if (response.data) {
                if (response.data.data.FeedList) {
                  var feedItemList = response.data.data.FeedList.pxResults;

                  if (Array.isArray(feedItemList)) {
                    feedItemList.forEach(function (message) {
                      message.ID = message.pzInsKey;
                      respMessageIDs.push(message.ID);
                      respMessages[message.ID] = message;
                    });
                  }
                }

                if (!feedFilters) {
                  feedFilters = [{
                    id: "All",
                    label: "All",
                    on: "true",
                    disabled: false
                  }];

                  if (response.data.data.FeedFilters) {
                    var feedFiltersList = response.data.data.FeedFilters.pxResults;
                    var allFilter = "true";

                    if (Array.isArray(feedFiltersList)) {
                      feedFiltersList.forEach(function (feedFilter) {
                        var feedFilterItem = {
                          id: feedFilter.pyFeedSourceReference,
                          label: feedFilter.pyLabel,
                          disabled: false,
                          on: feedFilter.pySelected
                        };
                        if (!feedFilterItem.on) allFilter = false;
                        feedFilters.push(feedFilterItem);
                      });
                      feedFilters.find(function (feedItem) {
                        return feedItem.id === "All";
                      }).on = allFilter;
                    }
                  }
                }
              } // if filters appllied, need to reset store


              if (filterBy !== "" || !pConnect.getValue("pulse.messageIDs")) pConnect.updateState({
                pulse: {
                  messages: respMessages,
                  messageIDs: respMessageIDs,
                  feedViewResponse: feedViewResponse
                }
              });else pConnect.updateState({
                pulse: {
                  messages: _objectSpread(_objectSpread({}, pConnect.getValue("pulse.messages")), respMessages),
                  messageIDs: [].concat(_toConsumableArray(pConnect.getValue("pulse.messageIDs")), respMessageIDs),
                  feedViewResponse: feedViewResponse
                }
              });
              return feedFilters;
            }

            pConnect.reportError("fetchMessages call failed ".concat(response.status), response.data);
            return "";
          })["catch"](function (error) {
            if (!isRequestCanceled(error)) {
              pConnect.reportError(": Error ocurred during ajax call at fetchMessages API : ", error.response);
            }
          });
        };

        var postMessage = function postMessage(pulseContext, message, attachmentIDs) {
          var isReply = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : false;
          pulseContext = getPulseContext(pulseContext);
          var reqBody = JSON.stringify({
            context: pulseContext,
            message: message,
            attachments: attachmentIDs
          });
          invokeRestApi("postFeedMessages", {
            body: reqBody
          }, pConnect.getContextName()).then(function (response) {
            if (response.status === 201 && response.data) {
              var messageData = response.data;
              var messageObject = {
                ID: messageData.ID,
                pzInsKey: messageData.ID,
                pyFeed: {
                  pyPostedOn: messageData.postedTime,
                  pyFeedTitle: messageData.postedByUser.name,
                  pyCommentContext: "pzInsKey",
                  pyIconType: "user",
                  pyIconReference: "pi pi-case"
                },
                postedByUser: messageData.postedByUser,
                pyMessage: messageData.message,
                mentions: messageData.mentions || [],
                pyMessageViewReference: "pzPostDetails",
                pxIcon: "globe",
                tags: messageData.tags || []
              };

              if (isReply) {
                var pxResults = pConnect.getValue(".pxResults");
                if (!pxResults) pxResults = [];
                pConnect.setValue(".pxResults", [].concat(_toConsumableArray(pxResults), [messageObject]));
              } else {
                messageObject.pxResults = [];

                if (pConnect.getValue("pulse.messageIDs")) {
                  var messageIDs = pConnect.getValue("pulse.messageIDs");
                  var messages = pConnect.getValue("pulse.messages");
                  pConnect.updateState({
                    pulse: {
                      messages: _objectSpread(_objectSpread({}, messages), {}, _defineProperty({}, messageObject.ID, messageObject)),
                      messageIDs: [messageObject.ID].concat(_toConsumableArray(messageIDs))
                    }
                  });
                } else {
                  pConnect.updateState({
                    pulse: {
                      messages: _defineProperty({}, messageObject.ID, messageObject),
                      messageIDs: [messageObject.ID]
                    }
                  });
                }
              }
            } else {
              pConnect.reportError("postMessage call failed with status ".concat(response.status), response);
            }
          })["catch"](function (error) {
            pConnect.reportError(": Error ocurred during ajax call at postMessage API : ", error.response);
          });
        };

        var likeMessage = function likeMessage(_ref8) {
          var pulseContext = _ref8.pulseContext,
              unLiked = _ref8.likedBy,
              messageID = _ref8.messageID,
              isReply = _ref8.isReply;
          pulseContext = getPulseContext(pulseContext);
          var routeKey = unLiked ? "unlikeFeedMessages" : "likeFeedMessages";
          var body = JSON.stringify({
            ContextClass: pulseContext
          });
          var queryPayload = {
            pulseContext: pulseContext
          };
          invokeRestApi(routeKey, {
            body: body,
            queryPayload: queryPayload
          }, pConnect.getContextName()).then(function (response) {
            if (response.status === 200) {
              if (isReply) {
                var msg = pConnect.getValue("pulse.messages.".concat(messageID));
                var pxResults = msg.pxResults.map(function (reply) {
                  reply = _objectSpread({}, reply);

                  if (reply.pzInsKey === pulseContext) {
                    var updatedLikeCount = 0;
                    var updatedLikedFlag = false;
                    var likeCount = reply.pyLikes ? reply.pyLikes.pxLikeCount : 0;

                    if (unLiked) {
                      updatedLikeCount = likeCount - 1;
                      updatedLikedFlag = false;
                    } else {
                      updatedLikeCount = likeCount + 1;
                      updatedLikedFlag = true;
                    }

                    reply.pyLikes = {
                      pxLikeCount: updatedLikeCount,
                      pxIsLiked: updatedLikedFlag
                    };
                  }

                  return reply;
                });
                pConnect.updateState({
                  pulse: {
                    messages: _defineProperty({}, messageID, {
                      pxResults: pxResults
                    })
                  }
                });
              } else {
                var _msg = _objectSpread({}, pConnect.getValue("pulse.messages.".concat(messageID)));

                var likeCount = _msg.pyLikes ? _msg.pyLikes.pxLikeCount : 0;
                var updatedLikeCount = 0;
                var updatedLikedFlag = false;

                if (unLiked) {
                  updatedLikeCount = likeCount - 1;
                  updatedLikedFlag = false;
                } else {
                  updatedLikeCount = likeCount + 1;
                  updatedLikedFlag = true;
                }

                _msg.pyLikes = {
                  pxLikeCount: updatedLikeCount,
                  pxIsLiked: updatedLikedFlag
                };
                pConnect.updateState({
                  pulse: {
                    messages: _defineProperty({}, pulseContext, _msg)
                  }
                });
              }
            } else {
              pConnect.reportError("likeMessage call failed with status ".concat(response.status), response);
            }
          })["catch"](function (error) {
            pConnect.reportError(": Error ocurred during ajax call at likeMessage API : ", error.response.data);
          });
        };

        var deleteMessage = function deleteMessage(messageID, isReply, replyID) {
          var messageKey = messageID;

          if (isReply) {
            messageKey = replyID;
          }

          var queryPayload = {
            messageID: messageKey
          };
          invokeRestApi("deleteFeedMessage", {
            queryPayload: queryPayload
          }, pConnect.getContextName()).then(function (response) {
            if (response.status === 200) {
              if (isReply) {
                var msg = pConnect.getValue("pulse.messages.".concat(messageID));
                var pxResults = msg.pxResults.filter(function (reply) {
                  return reply.pzInsKey !== replyID;
                });
                pConnect.updateState({
                  pulse: {
                    messages: _defineProperty({}, messageID, {
                      pxResults: pxResults
                    })
                  }
                });
              } else {
                var msgIDs = pConnect.getValue("pulse.messageIDs");
                var newMsgIDs = msgIDs.filter(function (msgID) {
                  return msgID !== messageID;
                });

                var msgs = _objectSpread({}, pConnect.getValue("pulse.messages"));

                delete msgs[messageID];
                pConnect.updateState({
                  pulse: {
                    messageIDs: newMsgIDs,
                    messages: msgs
                  }
                });
              }
            } else {
              pConnect.reportError("deleteMessage call failed with status ".concat(response.status), response);
            }
          })["catch"](function (error) {
            pConnect.reportError(": Error ocurred during ajax call at deleteMessage API : ", error.response);
          });
        };

        var getMentionSuggestions = function getMentionSuggestions(mentionProps) {
          var searchFor = mentionProps.searchFor,
              _mentionProps$mention = mentionProps.mentionsType,
              mentionsType = _mentionProps$mention === void 0 ? "Users" : _mentionProps$mention,
              _mentionProps$listSiz = mentionProps.listSize,
              listSize = _mentionProps$listSiz === void 0 ? 5 : _mentionProps$listSiz;
          var pulseContext = getPulseContext(mentionProps.pulseContext);
          var queryPayload = {
            pulseContext: pulseContext,
            searchFor: searchFor,
            mentionsType: mentionsType,
            listSize: listSize
          };

          for (var i = 0; i < mentionsTagsCancelTokenSource.length; i += 1) {
            mentionsTagsCancelTokenSource[i].cancel();
          }

          var newCancelTokenSource = getCancelTokenSource();
          mentionsTagsCancelTokenSource.push(newCancelTokenSource);
          return invokeRestApi("getMentionSuggestions", {
            queryPayload: queryPayload,
            cancelTokenSource: newCancelTokenSource
          }, pConnect.getContextName()).then(function (response) {
            mentionsTagsCancelTokenSource = [];
            var mentionSuggestions = [];

            if (response.status === 200 && response.data) {
              mentionSuggestions = response.data.map(function (mentionSuggestion) {
                return {
                  fullname: mentionSuggestion.caption,
                  id: mentionSuggestion.mentionsID
                };
              });
            } else {
              pConnect.reportError("Get mention suggestions call failed ".concat(response.status), response.data);
            }

            return mentionSuggestions;
          })["catch"](function (err) {
            if (!isRequestCanceled(err)) {
              pConnect.reportError(": Error ocurred during ajax call at getMentionSuggestions API : ", err.response.data);
            }
          });
        };

        var getTagSuggestions = function getTagSuggestions(mentionProps) {
          var searchFor = mentionProps.searchFor,
              _mentionProps$listSiz2 = mentionProps.listSize,
              listSize = _mentionProps$listSiz2 === void 0 ? 5 : _mentionProps$listSiz2;
          var queryPayload = {
            searchFor: searchFor,
            listSize: listSize
          };

          for (var i = 0; i < mentionsTagsCancelTokenSource.length; i += 1) {
            mentionsTagsCancelTokenSource[i].cancel();
          }

          var newCancelTokenSource = getCancelTokenSource();
          mentionsTagsCancelTokenSource.push(newCancelTokenSource);
          return invokeRestApi("getTagSuggestions", {
            queryPayload: queryPayload,
            cancelTokenSource: newCancelTokenSource
          }, pConnect.getContextName()).then(function (response) {
            mentionsTagsCancelTokenSource = [];
            var tagSuggestions = [];

            if (response.status === 200 && response.data) {
              tagSuggestions = response.data.tags.map(function (tag) {
                return "#".concat(tag);
              });
            } else {
              pConnect.reportError("Get tag suggestions call failed ".concat(response.status), response.data);
            }

            return tagSuggestions;
          })["catch"](function (err) {
            if (!isRequestCanceled(err)) {
              pConnect.reportError(": Error ocurred during ajax call at getTagSuggestions API : ", err.response.data);
            }
          });
        };

        return {
          fetchMessages: fetchMessages,
          postMessage: postMessage,
          likeMessage: likeMessage,
          deleteMessage: deleteMessage,
          getMentionSuggestions: getMentionSuggestions,
          getTagSuggestions: getTagSuggestions
        };
      };
      /* harmony default export */


