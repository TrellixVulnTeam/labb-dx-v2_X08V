              var bPortalLogin = !_this16.authIsEmbedded();

              if (bPortalLogin && !bFullReauth) {
                // update redirect uri to be the root
                _this16.updateRedirectUri("".concat(window.location.origin).concat(window.location.pathname));

                _this16.authMgr.loginRedirect(); // Don't have token til after the redirect


                return Promise.resolve(undefined);
              } else {
                _this16.updateRedirectUri("".concat(window.location.origin, "/auth.html"));

                return new Promise(function (resolve, reject) {
                  _this16.authMgr.login().then(function (token) {
                    _this16.processTokenOnLogin(token, true);

                    resolve(token.access_token);
                  })["catch"](function (e) {
                    sessionStorage.removeItem("asdk_loggingIn"); //console.log("login(): clearing loggingIn SS")
                    // eslint-disable-next-line no-console

                    console.error("Error caught during login: ".concat(e));
                    reject(e);
                  });
                });
              }
            });
          });
        };
        /**
         * Silent or visible login based on login status
         */


        this.loginIfNecessary = function (appName) {
          var isEmbedded = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;
          var deferLogin = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;

          // If embedded status of page changed...clearAuthMgr
