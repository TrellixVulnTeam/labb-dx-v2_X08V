                      //  normalize it to get the referencedView if that's the case
                      var theNewPConn = _reference_reference_component__WEBPACK_IMPORTED_MODULE_8__["ReferenceComponent"].normalizePConn(rootObject.getPConnect()); // update ComponentName$ before we update pConn$ to make sure they're in sync
