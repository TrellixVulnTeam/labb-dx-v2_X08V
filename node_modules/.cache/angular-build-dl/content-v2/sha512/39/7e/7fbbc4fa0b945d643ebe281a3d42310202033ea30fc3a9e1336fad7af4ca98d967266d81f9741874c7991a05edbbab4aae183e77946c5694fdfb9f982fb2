    function h(module, __webpack_exports__, __webpack_require__) {
      "use strict";

      __webpack_require__.r(__webpack_exports__);
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "TABLE_CELL", function () {
        return TABLE_CELL;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "DELETE_ICON", function () {
        return DELETE_ICON;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "getContext", function () {
        return getContext;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "populateRowKey", function () {
        return populateRowKey;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "getApiContext", function () {
        return getApiContext;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "buildMetaForListView", function () {
        return buildMetaForListView;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "buildFieldsForTable", function () {
        return buildFieldsForTable;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "createMetaForTable", function () {
        return createMetaForTable;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "getAddRowCallback", function () {
        return getAddRowCallback;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "createPConnect", function () {
        return createPConnect;
      });

      var TABLE_CELL = "SdkRenderer";
      var DELETE_ICON = "DeleteIcon"; // BUG-615253: Workaround for autosize in table with lazy loading components

      /* istanbul ignore next */

      function getFiledWidth(field, label) {
        var width;

        switch (field.type) {
          case "Time":
            width = 150;
            break;

          case "Date":
            width = 160;
            break;

          case "DateTime":
            width = 205;
            break;

          case "AutoComplete":
          case "TextArea":
            width = 190;
            break;

          case "Currency":
          case "TextInput":
            width = 182;
            break;

          case "Checkbox":
            // eslint-disable-next-line no-case-declarations
            var text = document.createElement("span");
            document.body.appendChild(text);
            text.style.fontSize = "13px";
            text.style.position = "absolute";
            text.innerHTML = label;
            width = Math.ceil(text.clientWidth) + 30;
            document.body.removeChild(text);
            break;

          default:
            width = 180;
        }

        return width;
      }

      var getContext = function getContext(thePConn) {
        var _a;

        var contextName = thePConn.getContextName();
        var pageReference = thePConn.getPageReference(); // 8.7 change = referenceList may now be in top-level of state props, 
        //  not always in config of state props

        var _ref = ((_a = thePConn.getStateProps()) === null || _a === void 0 ? void 0 : _a.config) || thePConn.getStateProps(),
            referenceList = _ref.referenceList;

        var pageReferenceForRows = referenceList.startsWith(".") ? "".concat(pageReference, ".").concat(referenceList.substring(1)) : referenceList; // removing "caseInfo.content" prefix to avoid setting it as a target while preparing pageInstructions

        referenceList = pageReferenceForRows.replace(PCore.getConstants().CASE_INFO.CASE_INFO_CONTENT, "");
        return {
          contextName: contextName,
          referenceListStr: referenceList,
          pageReferenceForRows: pageReferenceForRows
        };
      };

      var populateRowKey = function populateRowKey(rawData) {
        return rawData.map(function (row, index) {
          return Object.assign(Object.assign({}, row), {
            index: index
          });
        });
      };

      var getApiContext = function getApiContext(processedData, pConnect, reorderCB) {
        return {
          fetchData: function fetchData() {
            return new Promise(function (resolve) {
              resolve({
                data: processedData,
                filteredRecordCount: processedData.length,
                totalRecordCount: processedData.length
              });
            });
          },
          fetchPersonalizations: function fetchPersonalizations() {
            return Promise.resolve({});
          },
          applyRowReorder: function applyRowReorder(sourceKey, destinationKey) {
            // indexes are keys for simple table so, it should work.
            reorderCB();
            return Promise.resolve(pConnect.getListActions().reorder(parseInt(sourceKey, 10), parseInt(destinationKey, 10)));
          }
        };
      };

      var buildMetaForListView = function buildMetaForListView(fieldMetadata, fields, type, ruleClass, name, propertyLabel, parameters) {
        return {
          name: name,
          config: {
            type: type,
            referenceList: fieldMetadata.datasource.name,
            parameters: parameters !== null && parameters !== void 0 ? parameters : fieldMetadata.datasource.parameters,
            personalization: false,
            grouping: true,
            globalSearch: true,
            reorderFields: true,
            toggleFieldVisibility: true,
            personalizationId: ""
            /* TODO */
            ,
            template: "ListView",
            presets: [{
              name: "presets",
              template: "Table",
              config: {},
              children: [{
                name: "Columns",
                type: "Region",
                children: fields
              }],
              label: propertyLabel,
              id: "P_"
              /* TODO */

            }],
            ruleClass: ruleClass
          }
        };
      };

      var buildFieldsForTable = function buildFieldsForTable(configFields, fields, bReadOnly) {
        var fieldDefs = configFields.map(function (field, index) {
          return {
            type: "text",
            label: fields[index].config.label || fields[index].config.caption,
            fillAvailableSpace: !!field.config.fillAvailableSpace,
            id: index,
            name: field.config.value.substr(4),
            cellRenderer: TABLE_CELL,
            sort: false,
            noContextMenu: true,
            showMenu: false,
            meta: Object.assign({}, field),
            // BUG-615253: Workaround for autosize in table with lazy loading components
            width: getFiledWidth(field, fields[index].config.label)
          };
        }); // ONLY add DELETE_ICON to fields when the table is requested as EDITABLE

        if (!bReadOnly) {
          fieldDefs.push({
            type: "text",
            id: fieldDefs.length,
            cellRenderer: DELETE_ICON,
            sort: false,
            noContextMenu: true,
            showMenu: false,
            // BUG-615253: Workaround for autosize in table with lazy loading components
            width: 46
          });
        }

        return fieldDefs;
      };

      var createMetaForTable = function createMetaForTable(fields, renderMode) {
        return {
          height: {
            minHeight: "auto",
            fitHeightToElement: "fitHeightToElement",
            deltaAdjustment: "deltaAdjustment",
            autoSize: true
          },
          fieldDefs: fields,
          itemKey: "index",
          grouping: false,
          reorderFields: false,
          reorderItems: renderMode === "Editable",
          dragHandle: renderMode === "Editable",
          globalSearch: false,
          personalization: false,
          toggleFieldVisibility: false,
          toolbar: false,
          footer: false,
          filterExpression: null,
          editing: false,
          timezone: PCore.getEnvironmentInfo().getTimeZone()
        };
      };
      /**
       * This method returns a callBack function for Add action.
       * @param {object} pConnect - PConnect object
       * @param {number} index - index of the page list to add
       */


      var getAddRowCallback = function getAddRowCallback(pConnect, index) {
        return function () {
          return pConnect.getListActions().insert({}, index);
        };
      };
      /**
       * This method creates a PConnect object with proper options for Add and Delete actions
       * @param {string} contextName - contextName
       * @param {string} referenceList - referenceList
       * @param {string} pageReference - pageReference
