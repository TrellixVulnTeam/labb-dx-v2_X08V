        }, {
          key: "addGroups",
          value: function addGroups(data, groupByColumns) {
            var rootGroup = new Group();
            rootGroup.expanded = true;
            return this.getSublevel(data, 0, groupByColumns, rootGroup);
          }
        }, {
          key: "getSublevel",
          value: function getSublevel(data, level, groupByColumns, parent) {
            var _this57 = this;

            if (level >= groupByColumns.length) {
              return data;
            }

            var groups = this.uniqueBy(data.map(function (row) {
              var result = new Group();
              result.level = level + 1;
              result.parent = parent;

              for (var i = 0; i <= level; i++) {
                result[groupByColumns[i]] = row[groupByColumns[i]];
              }

              return result;
            }), JSON.stringify);
            var currentColumn = groupByColumns[level];
            var subGroups = [];
            groups.forEach(function (group) {
              var rowsInGroup = data.filter(function (row) {
                return group[currentColumn] === row[currentColumn];
              });
              group.totalCounts = rowsInGroup.length;

              var subGroup = _this57.getSublevel(rowsInGroup, level + 1, groupByColumns, group);

              subGroup.unshift(group);
              subGroups = subGroups.concat(subGroup);
            });
            return subGroups;
          }
        }, {
          key: "uniqueBy",
          value: function uniqueBy(a, key) {
            var seen = {};
            return a.filter(function (item) {
              var k = key(item);
              return seen.hasOwnProperty(k) ? false : seen[k] = true;
            });
          }
        }, {
