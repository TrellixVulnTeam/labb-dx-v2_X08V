        return compareSdkPCoreVersions;
      });
      /**
       * versionHelpers.ts
       *
       * Container helper functions that can identify which version of
       * PCore/PConnect is being run
       */


      var sdkVersion = "8.7";

      function compareSdkPCoreVersions() {
        var theConstellationVersion = PCore.getPCoreVersion();

        if (theConstellationVersion.includes(sdkVersion)) {
          // eslint-disable-next-line no-console
          console.log("sdkVersion: ".concat(sdkVersion, " matches PCore version: ").concat(PCore.getPCoreVersion()));
        } else {
          // eslint-disable-next-line no-console
          console.error("sdkVersion: ".concat(sdkVersion, " does NOT match PCore version: ").concat(PCore.getPCoreVersion()));
        }
      }
