        }, {
          key: "generateDate",
          value: function generateDate(dateVal, dateFormat) {
            var sReturnDate = dateVal;

            if (dateVal == null || dateVal == "") {
              return dateVal;
            } // Commenting out per Vinod note in webc components
            // VRS: Dont think we need the below line.  Commenting out for now. (2020.05.20)
            // if date has a ".", then of the format YYYMMDD[T]HHmmss[.]SSS Z, need to change to YYYYMMDD
            // if (dateVal.indexOf(".") >= 0) {
            //   dateVal = moment(dateVal.replace("GMT", "+0000"), "YYYYMMDD[T]HHmmss[.]SSS Z").format("YYYYMMDD");
            //   sReturnDate = dateVal;
            // }


            switch (dateFormat) {
              case "Date-Short":
                // 1/1/01
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("M/D/YY");
                break;

              case "Date-Short-YYYY":
                // 1/1/2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("M/D/YYYY");
                break;

              case "Date-Short-Custom":
                // 01/01/01
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("MM/DD/YY");
                break;

              case "Date-Short-Custom-YYYY":
                // 01/01/2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("L");
                break;

              case "Date-Long-Custom-YYYY":
                // 05/22/2021
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("MM/DD/YYYY");
                break;

              case "Date":
              case "Date-Medium":
                // Jan 1, 2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("ll");
                break;

              case "Date-DayMonthYear-Custom":
                // 01-Jan-2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("DD-MMM-YYYY");
                break;

              case "Date-Full":
                // Monday, January 1, 2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("dddd, MMMM D, YYYY");
                break;

              case "DateTime-Frame":
              case "DateTime-Frame-Short":
              case "DateTime-Since":
                // 2 days, 5 hours ago
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).fromNow();
                break;

              case "Date-Long":
                // January 1, 2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("MMMM D, YYYY");
                break;

              case "Date-ISO-8601":
                // 2001/01/01 y/m/d
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("YYYY/MM/DD");
                break;

              case "Date-Gregorian-1":
                // 01 January, 2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("DD MMMM, YYYY");
                break;

              case "Date-Gregorian-2":
                // January 01, 2001
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("MMMM DD, YYYY");
                break;

              case "Date-Gregorian-3":
                // 2001, January 01
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateVal).format("YYYY, MMMM DD");
                break;

              case "DateTime-Custom":
                break;
            }

            return sReturnDate;
          }
        }, {
          key: "generateDateTime",
          value: function generateDateTime(dateTimeVal, dateFormat) {
            var sReturnDate = dateTimeVal;

            if (dateTimeVal == null || dateTimeVal == "") {
              return dateTimeVal;
            }

            dateTimeVal = dateTimeVal.replace("GMT", "+0000");

            switch (dateFormat) {
              case "DateTime-Short":
                // 1/1/01 1:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("M/D/YY h:mm A");
                break;

              case "DateTime-Short-Custom":
                // 01/01/01 01:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("MM/DD/YY hh:mm A");
                break;

              case "DateTime-Short-YYYY-Custom":
                // 01/01/2001 01:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("M/D/YYYY hh:mm A");
                break;

              case "DateTime-Long-YYYY-Custom":
                // 01/01/01 01:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("MM/DD/YYYY, hh:mm A");
                break;

              case "DateTime-Short-YYYY":
                // 1/1/2001 1:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("M/D/YYYY h:mm A");
                break;

              case "DateTime-Medium":
                // Jan 1, 2001 1:00:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("MMM D, YYYY h:mm:ss A");
                break;

              case "DateTime-Long":
                // January 1, 2001 1:00:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("MMMM D, YYYY h:mm:ss A");
                break;

              case "DateTime-DayMonthYear-Custom":
                // 01-Jan-2001 1:00:00 AM
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("DD-MMM-YYYY h:mm:ss A");
                break;

              case "DateTime-Full":
                // Monday, January 1, 2001 1:00 AM EDT
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("dddd, MMMM D, YYYY h:mm A z");
                break;

              case "DateTime-Frame":
              case "DateTime-Frame-Short":
              case "DateTime-Since":
                // 2 days, 5 hours ago
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).fromNow();
                break;

              case "DateTime-ISO-8601":
                // 2001/01/01 1:00:00 AM     y/m/d
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("YYYY/MM/DD h:mm:ss A");
                break;

              case "DateTime-Gregorian-1":
                // 01 January, 2001 1:00:00 AM 
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("DD MMMM, YYYY h:mm:ss A");
                break;

              case "DateTime-Gregorian-2":
                // January 01, 2001 1:00:00 AM 
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("MMMM DD, YYYY h:mm:ss A");
                break;

              case "DateTime-Gregorian-3":
                // 2001, January 01 1:00:00 AM 
                sReturnDate = dayjs__WEBPACK_IMPORTED_MODULE_2__(dateTimeVal).format("YYYY, MMMM DD h:mm:ss A");
                break;

              case "DateTime-Custom":
                break;
            }

            return sReturnDate;
          }
        }, {
          key: "getSeconds",
          value: function getSeconds(sTime) {
            return dayjs__WEBPACK_IMPORTED_MODULE_2__(sTime).valueOf();
          }
        }, {
          key: "getIconFromFileType",
          value: function getIconFromFileType(fileType) {
            var icon = "document-doc";
            if (!fileType) return icon;

            if (fileType.startsWith("audio")) {
              icon = "audio";
            } else if (fileType.startsWith("video")) {
              icon = "video";
            } else if (fileType.startsWith("image")) {
              icon = "picture";
            } else if (fileType.includes("pdf")) {
              icon = "document-pdf";
            } else {
              var _fileType$split = fileType.split("/"),
                  _fileType$split2 = _slicedToArray(_fileType$split, 2),
                  subtype = _fileType$split2[1];

              var foundMatch = function foundMatch(sources) {
                return sources.some(function (key) {
                  return subtype.includes(key);
                });
              };

              if (foundMatch(["excel", "spreadsheet"])) {
                icon = "document-xls";
              } else if (foundMatch(["zip", "compressed", "gzip", "rar", "tar"])) {
                icon = "document-compress";
              }
            }

            return icon;
          }
        }, {
          key: "getIconForAttachment",
          value: function getIconForAttachment(attachment) {
            var icon;

            switch (attachment.type) {
              case "FILE":
                icon = this.getIconFromFileType(attachment.mimeType);
                break;

              case "URL":
                icon = "chain";
                break;

              default:
                icon = "document-doc";
            }

            return icon;
          }
        }, {
          key: "addViewContainer",
          value: function addViewContainer() {
            if (sessionStorage.getItem("viewContainerCount") == null) {
              sessionStorage.setItem("viewContainerCount", "0");
            }

            var count = parseInt(sessionStorage.getItem("viewContainerCount"));
            count++;
            sessionStorage.setItem("viewContainerCount", count.toString());
          }
        }, {
          key: "removeViewContainer",
          value: function removeViewContainer() {
            return;
            var count = parseInt(sessionStorage.getItem("viewContainerCount"));
            count--;
            sessionStorage.setItem("viewContainerCount", count.toString());
          }
        }, {
          key: "okToAddContainerToVC",
          value: function okToAddContainerToVC() {
            if (sessionStorage.getItem("viewContainerCount") == null) {
              sessionStorage.setItem("viewContainerCount", "0");
            }

            var count = parseInt(sessionStorage.getItem("viewContainerCount"));

            if (count == 0) {
              return true;
            } else {
              return false;
            }
          }
        }]);

        return Utils;
      }();

