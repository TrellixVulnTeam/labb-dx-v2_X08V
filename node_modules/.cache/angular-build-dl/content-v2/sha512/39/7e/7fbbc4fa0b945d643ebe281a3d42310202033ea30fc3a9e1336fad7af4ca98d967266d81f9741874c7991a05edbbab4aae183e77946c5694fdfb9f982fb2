        function TextContentComponent(angularPConnect, utils) {
          _classCallCheck(this, TextContentComponent);

          this.angularPConnect = angularPConnect;
          this.utils = utils;
          this.content$ = ""; // Used with AngularPConnect

          this.angularPConnectData = {};
        }

        _createClass(TextContentComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            // First thing in initialization is registering and subscribing to the AngularPConnect service
            this.angularPConnectData = this.angularPConnect.registerAndSubscribeComponent(this, this.onStateChange); // Then, continue on with other initialization
            // call updateSelf when initializing
            //this.updateSelf();

            this.checkAndUpdate();
          }
        }, {
          key: "ngOnDestroy",
          value: function ngOnDestroy() {
            if (this.angularPConnectData.unsubscribeFn) {
              this.angularPConnectData.unsubscribeFn();
            }
          } // updateSelf

        }, {
          key: "updateSelf",
          value: function updateSelf() {
            // moved this from ngOnInit() and call this from there instead...
            this.configProps$ = this.pConn$.resolveConfigProps(this.pConn$.getConfigProps());

            if (this.configProps$["content"] != undefined) {
              this.content$ = this.configProps$["content"];
            }

            if (this.configProps$["displayAs"] != undefined) {
              this.displayAs$ = this.configProps$["displayAs"];
            } // Any update to content or displayAs will re-render this component.
            //  All rendering logic is in the .html file.

          } // Callback passed when subscribing to store change

        }, {
          key: "onStateChange",
          value: function onStateChange() {
            this.checkAndUpdate();
          }
        }, {
          key: "checkAndUpdate",
          value: function checkAndUpdate() {
            // Should always check the bridge to see if the component should
            // update itself (re-render)
            var bUpdateSelf = this.angularPConnect.shouldComponentUpdate(this); // ONLY call updateSelf when the component should update

            if (bUpdateSelf) {
              this.updateSelf();
            }
          }
        }]);

        return TextContentComponent;
      }();

