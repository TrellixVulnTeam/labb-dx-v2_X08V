        this.getAuthMgr = function (bInit) {
          return new Promise(function (resolve) {
            var idNextCheck = null;

            var fnCheckForAuthMgr = function fnCheckForAuthMgr() {
              if (_helpers_auth__WEBPACK_IMPORTED_MODULE_3__["default"] && !_this16.authMgr) {
                _this16.initAuthMgr(bInit);
              }

              if (_this16.authMgr) {
                if (idNextCheck) {
                  clearInterval(idNextCheck);
                }

                return resolve(_this16.authMgr);
              }
            };

            idNextCheck = setInterval(fnCheckForAuthMgr, 500);
          });
        };

        this.login = function () {
          var bFullReauth = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : false;

          _this16.scservice.getServerConfig().then(function () {
            // Needed so a redirect to login screen and back will know we are still in process of logging in
            sessionStorage.setItem("asdk_loggingIn", "".concat(Date.now())); //console.log("loggingIn: setting time");

            _this16.getAuthMgr(!bFullReauth).then(function (aMgr) {
