          var currAppName = sessionStorage.getItem("asdk_appName");

          if (appName !== currAppName || isEmbedded !== currEmbedded) {
            _this16.clearAuthMgr();

            sessionStorage.setItem("asdk_appName", appName);
          }

          _this16.setIsEmbedded(isEmbedded); // If this is the login redirect with auth code


          if (window.location.href.indexOf("?code") !== -1) {
            // initialize authMgr
            _this16.initAuthMgr(false);

            sessionStorage.removeItem("asdk_loggingIn"); //console.log("loginIfNecessary: clearing loggingIn");

            _this16.authRedirectCallback(window.location.href, function () {
              //sessionStorage.removeItem("asdk_loggingIn");
              console.log("loginIfNecessary: clearing loggingIn (commented out)");
              window.location.href = window.location.pathname;
            });

            return;
          } // If not login redirect with auth code


          if (!deferLogin && (!_this16.isLoginInProgress() || _this16.isLoginExpired())) {
            _this16.getAuthMgr(false).then(function () {
              _this16.updateLoginStatus();

              if (_this16.bLoggedIn) {
                _this16.fireTokenAvailable(_this16.getCurrentTokens());
              } else {
                return _this16.login();
              }
            });
          }
        };

        this.fullReauth = function () {
          var bHandleHere = true; // Other alternative is to raise an event and have someone else handle it

          if (bHandleHere) {
            // Don't want to do a full clear of authMgr as will loose sessionIndex.  Rather just clear the tokens
            _this16.clearAuthMgr(true);

            _this16.login(true);
          } else {
            // Create and dispatch the SdkLoggedIn event to trigger constellationInit
            // detail will be callback function to call once a new token structure is obtained
            var event = new CustomEvent('SdkFullReauth', {
              detail: _this16.processTokenOnLogin.bind(_this16)
            });
            document.dispatchEvent(event);
          }
        };

        this.logout = function () {
          sessionStorage.removeItem("authHdr");

          var tokenInfo = _this16.getCurrentTokens();

          if (tokenInfo && tokenInfo.access_token) {
            if (window.PCore) {
              window.PCore.getAuthUtils().revokeTokens().then(function () {
                _this16.clearAuthMgr();
              })["catch"](function (err) {
                // eslint-disable-next-line no-console
                console.log("Error:", err === null || err === void 0 ? void 0 : err.message);
              });
            } else {
              _this16.authMgr.revokeTokens(tokenInfo.access_token, tokenInfo.refresh_token).then(function () {
                _this16.clearAuthMgr();
              });
            }
          }
        };

        this.scservice.getServerConfig().then(function () {});
      });

