          key: "clearOutArrows",
          value: function clearOutArrows(event, columnData) {
            var arImages = event.srcElement.parentElement.getElementsByTagName("img");

            var _iterator16 = _createForOfIteratorHelper(arImages),
                _step16;

            try {
              for (_iterator16.s(); !(_step16 = _iterator16.n()).done;) {
