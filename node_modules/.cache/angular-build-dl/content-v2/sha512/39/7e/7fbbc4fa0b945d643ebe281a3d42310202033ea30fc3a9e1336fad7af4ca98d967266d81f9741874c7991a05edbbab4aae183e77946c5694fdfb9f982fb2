            if (bAssignmentsForThisOperator || hasChildCaseAssignments || this.isCaseWideLocalAction()) {
              hasAssignments = true;
            }

            return hasAssignments;
          }
        }, {
          key: "isCaseWideLocalAction",
          value: function isCaseWideLocalAction() {
            var actionID = this.pConn$.getValue(this.pCoreConstants.CASE_INFO.ACTIVE_ACTION_ID);
            var caseActions = this.pConn$.getValue(this.pCoreConstants.CASE_INFO.AVAILABLEACTIONS);
            var bCaseWideAction = false;

            if (caseActions && actionID) {
              var actionObj = caseActions.find(function (caseAction) {
                return caseAction.ID === actionID;
              });

              if (actionObj) {
                bCaseWideAction = actionObj.type === "Case";
              }
            }

            return bCaseWideAction;
          }
