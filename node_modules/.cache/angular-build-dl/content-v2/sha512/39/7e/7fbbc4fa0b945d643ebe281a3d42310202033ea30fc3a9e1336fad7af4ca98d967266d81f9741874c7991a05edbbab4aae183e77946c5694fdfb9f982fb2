          key: "topViewRefresh",
          value: function topViewRefresh() {
            Object.values(this.formGroup$.controls).forEach(function (control) {
              control.markAsTouched();
            });
          } // helpers - copyied from flow container helpers.js

        }, {
          key: "addContainerItem",
          value: function addContainerItem(pConnect) {
            // copied from flow container helper.js
            var containerManager = pConnect.getContainerManager();
            var contextName = pConnect.getContextName(); // here we will get parent context name, as flow container is child of view container

            var caseViewMode = pConnect.getValue("context_data.caseViewMode");
            var key;
            var flowName;

            if (caseViewMode !== "review") {
              var target = contextName.substring(0, contextName.lastIndexOf("_"));
              var activeContainerItemID = this.PCore$.getContainerUtils().getActiveContainerItemName(target);
              var containerItemData = this.PCore$.getContainerUtils().getContainerItemData(target, activeContainerItemID);

              if (containerItemData) {
                key = containerItemData.key;
                flowName = containerItemData.flowName;
              }
            }

            containerManager.addContainerItem({
              semanticURL: "",
              key: key,
              flowName: flowName,
              caseViewMode: "perform",
              resourceType: "ASSIGNMENT",
              data: pConnect.getDataObject(contextName)
            });
          }
        }, {
          key: "showTodo",
          value: function showTodo(pConnect) {
            var caseViewMode = pConnect.getValue("context_data.caseViewMode");
            return caseViewMode !== "perform";
          }
        }, {
          key: "getChildCaseAssignments",
          value: function getChildCaseAssignments(pConnect) {
            var childCases = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.CHILD_ASSIGNMENTS);
            var allAssignments = [];

            if (childCases && childCases.length > 0) {
              childCases.forEach(function (_ref3) {
                var _ref3$assignments = _ref3.assignments,
                    assignments = _ref3$assignments === void 0 ? [] : _ref3$assignments,
                    Name = _ref3.Name;
                var childCaseAssignments = assignments.map(function (assignment) {
                  return Object.assign(Object.assign({}, assignment), {
                    caseName: Name
                  });
                });
                allAssignments = allAssignments.concat(childCaseAssignments);
              });
            }

            return allAssignments;
          }
        }, {
          key: "getActiveCaseActionName",
          value: function getActiveCaseActionName(pConnect) {
            var caseActions = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.CASE_INFO_ACTIONS);
            var activeActionID = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.ACTIVE_ACTION_ID);
            var activeAction = caseActions.find(function (action) {
              return action.ID === activeActionID;
            });
            return (activeAction === null || activeAction === void 0 ? void 0 : activeAction.name) || "";
          }
        }, {
          key: "getToDoAssignments",
          value: function getToDoAssignments(pConnect) {
            var _this34 = this;

            var caseActions = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.CASE_INFO_ACTIONS);
            var assignmentLabel = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.ASSIGNMENT_LABEL);
            var assignments = pConnect.getValue(this.PCore$.getConstants().CASE_INFO.D_CASE_ASSIGNMENTS_RESULTS) || [];
            var childCasesAssignments = this.getChildCaseAssignments(pConnect) || [];
            var childCasesAssignmentsCopy = JSON.parse(JSON.stringify(childCasesAssignments));
            childCasesAssignmentsCopy = childCasesAssignmentsCopy.map(function (assignment) {
              assignment.isChild = true;
              return assignment;
            });
            var todoAssignments = [].concat(_toConsumableArray(assignments), _toConsumableArray(childCasesAssignmentsCopy));
            var todoAssignmentsCopy = JSON.parse(JSON.stringify(todoAssignments));

            if (caseActions && !this.showTodo(pConnect)) {
              todoAssignmentsCopy = todoAssignmentsCopy.map(function (assignment) {
                assignment.name = _this34.getActiveCaseActionName(pConnect) || assignmentLabel;
                return assignment;
              });
            }

            return todoAssignmentsCopy;
          }
        }]);

        return FlowContainerComponent;
      }();

