        }, {
          key: "hasAssignments",
          value: function hasAssignments() {
            var hasAssignments = false;
            var assignmentsList = this.pConn$.getValue(this.pCoreConstants.CASE_INFO.D_CASE_ASSIGNMENTS_RESULTS);
            var thisOperator = this.PCore$.getEnvironmentInfo().getOperatorIdentifier(); // 8.7 includes assignments in Assignments List that may be assigned to
            //  a different operator. So, see if there are any assignments for
            //  the current operator

            var bAssignmentsForThisOperator = false; // Bail if there is no assignmentsList

            if (!assignmentsList) {
              return hasAssignments;
            }

