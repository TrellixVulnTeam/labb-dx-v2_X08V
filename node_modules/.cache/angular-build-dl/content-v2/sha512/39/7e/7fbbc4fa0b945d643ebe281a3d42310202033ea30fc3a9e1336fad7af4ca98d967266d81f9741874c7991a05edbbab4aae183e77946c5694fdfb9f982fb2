            var acName = this.newPConn$.getContainerName();
            var itemID = baseContext.concat("/").concat(acName);

            if (sButtonType == "secondary") {
              var stepID = ""; // ???
              // need to handle cancel as this.cancel(dispatchInfo)
              //this.actionsAPI[sAction](dispatchInfo);

              switch (sAction) {
                case "navigateToStep":
                  this.erService.sendMessage("publish", "");

                  if (this.formValid()) {
                    this.bReInit = true;
                    this.psService.sendMessage(true);
                    var navigatePromise = this.navigateToStep("previous", this.itemKey$);
                    navigatePromise.then(function () {
                      _this93.updateChanges();

                      _this93.psService.sendMessage(false);
                    })["catch"](function () {
                      _this93.psService.sendMessage(false);
                    });
                  }

                  break;

                case "cancelAssignment":
                  this.bReInit = true;
                  this.erService.sendMessage("dismiss", ""); // check if create stage (modal)

                  if (this.isCreateStage$) {
                    var cancelPromise = this.cancelCreateStageAssignment(this.itemKey$);
                    cancelPromise.then(function () {
                      _this93.psService.sendMessage(false); // this.PCore$.getPubSubUtils().publish(
                      //   this.PCore$.getConstants().PUB_SUB_EVENTS.EVENT_CANCEL);

                    })["catch"](function () {
                      _this93.psService.sendMessage(false);
                    });
                  } else {
                    this.psService.sendMessage(true); // publish before cancel pressed, because
                    // cancel assignment happens "after" cancel assignment happens

                    this.PCore$.getPubSubUtils().publish("cancelPressed");

                    var _cancelPromise = this.cancelAssignment(this.itemKey$);

                    _cancelPromise.then(function () {
                      _this93.psService.sendMessage(false);

                      _this93.PCore$.getPubSubUtils().publish(_this93.PCore$.getConstants().PUB_SUB_EVENTS.EVENT_CANCEL);
                    })["catch"](function () {
                      _this93.psService.sendMessage(false);
                    });
                  }

                  break;

                default:
                  break;
              }
            } else if (sButtonType == "primary") {
              switch (sAction) {
                case "finishAssignment":
                  this.erService.sendMessage("publish", "");

                  if (this.formValid()) {
                    this.bReInit = true;
                    this.psService.sendMessage(true);
                    var finishPromise = this.finishAssignment(this.itemKey$); // JA - was itemID but Nebula/Constellation uses itemKey

                    finishPromise.then(function () {
                      _this93.psService.sendMessage(false);

                      _this93.updateChanges();
                    })["catch"](function () {
                      _this93.psService.sendMessage(false);
                    });
                  } else {
                    //let snackBarRef = this.snackBar.open("Please fix errors on form.",  "Ok");
                    this.erService.sendMessage("show", "Please fix errors on form.");
                  }

                  break;

                default:
                  break;
              }
            }
          }
