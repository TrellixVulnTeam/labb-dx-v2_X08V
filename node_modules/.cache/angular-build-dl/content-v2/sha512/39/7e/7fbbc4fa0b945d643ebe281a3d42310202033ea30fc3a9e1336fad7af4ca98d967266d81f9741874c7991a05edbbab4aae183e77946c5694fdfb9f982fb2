        return filterForFieldValueList;
      }); //  This file is adapted from Nebula/Constellation components/Templates/utils.js


      function getAllFields(pConnect) {
        var metadata = pConnect.getRawMetadata();
        var allFields = [];

        if (metadata.children && metadata.children.map) {
          allFields = metadata.children.map(function (fields) {
            var children = fields.children instanceof Array ? fields.children : [];
            return children.map(function (field) {
              return field.config;
            });
          });
        }

        return allFields;
      }

      function filterForFieldValueList(fields) {
        return fields.filter(function (_ref4) {
          var visibility = _ref4.visibility;
          return visibility !== false;
        }).map(function (_ref5) {
          var value = _ref5.value,
              label = _ref5.label;
          return {
            id: label.toLowerCase(),
            name: label,
            value: value
          };
        });
      }
      /***/

