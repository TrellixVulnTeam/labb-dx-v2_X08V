              this.PCore$ = window.PCore;
            }

            if (this.displayOnlyFA$) {
              // for when non modal
              this.bShowAsModal$ = false;
            } // First thing in initialization is registering and subscribing to the AngularPConnect service


            this.angularPConnectData = this.angularPConnect.registerAndSubscribeComponent(this, this.onStateChange);
            var baseContext = this.pConn$.getContextName();
            var acName = this.pConn$.getContainerName(); //for now, in general this should be overridden by updateSelf(), and not be blank

            if (this.itemKey$ === "") {
              this.itemKey$ = baseContext.concat("/").concat(acName);
            }

            var containerMgr = this.pConn$.getContainerManager();
            containerMgr.initializeContainers({
              type: "multiple"
            });

            var _this$PCore$$getConst5 = this.PCore$.getConstants(),
                CONTAINER_TYPE = _this$PCore$$getConst5.CONTAINER_TYPE,
                PUB_SUB_EVENTS = _this$PCore$$getConst5.PUB_SUB_EVENTS;

            this.angularPConnect.shouldComponentUpdate(this);
          }
        }, {
          key: "ngOnChanges",
          value: function ngOnChanges() {}
        }, {
          key: "ngOnDestroy",
          value: function ngOnDestroy() {
            if (this.angularPConnectData.unsubscribeFn) {
              this.angularPConnectData.unsubscribeFn();
            }

            var _this$PCore$$getConst6 = this.PCore$.getConstants(),
                CONTAINER_TYPE = _this$PCore$$getConst6.CONTAINER_TYPE,
                PUB_SUB_EVENTS = _this$PCore$$getConst6.PUB_SUB_EVENTS;

            this.PCore$.getPubSubUtils().unsubscribe(PUB_SUB_EVENTS.EVENT_SHOW_CANCEL_ALERT, PUB_SUB_EVENTS.EVENT_SHOW_CANCEL_ALERT
            /* Should be same unique string passed during subscription */
            );
            this.bSubscribed = false;
          } // Callback passed when subscribing to store change

        }, {
          key: "onStateChange",
          value: function onStateChange() {
            // Should always check the bridge to see if the component should
            // update itself (re-render)
            var bUpdateSelf = this.angularPConnect.shouldComponentUpdate(this); // ONLY call updateSelf when the component should update

            if (bUpdateSelf) {
              this.updateSelf();
            } else if (this.bShowModal$) {// right now onlu get one updated when initial diaplay.  So, once modal is up
              // let fall through and do a check with "compareCaseInfoIsDifferent" until fixed
              //this.updateSelf();
            }
          } // updateSelf

        }, {
          key: "updateSelf",
          value: function updateSelf() {
            var _this65 = this;

            // routingInfo was added as component prop in populateAdditionalProps
            var routingInfo = this.angularPConnect.getComponentProp(this, "routingInfo");
            this.routingInfoRef["current"] = routingInfo;
            var loadingInfo;

            try {
              loadingInfo = this.pConn$.getLoadingStatus();
            } catch (ex) {}

            var configProps = this.pConn$.resolveConfigProps(this.pConn$.getConfigProps());

            if (!loadingInfo) {// turn off spinner
              //this.psService.sendMessage(false);
            }

            if (routingInfo && !loadingInfo
            /* && this.bUpdate */
            ) {
              var currentOrder = routingInfo.accessedOrder;

              if (undefined == currentOrder) {
                return;
              }

              var currentItems = routingInfo.items;

              var _this$getKeyAndLatest = this.getKeyAndLatestItem(routingInfo),
                  key = _this$getKeyAndLatest.key,
                  latestItem = _this$getKeyAndLatest.latestItem;

              if (currentOrder.length > 0) {
                if (currentItems[key] && currentItems[key].view && Object.keys(currentItems[key].view).length > 0) {
                  var currentItem = currentItems[key];
                  var rootView = currentItem.view;
                  var context = rootView.config.context;
                  var config = {
                    meta: rootView
                  };
                  config["options"] = {
                    context: currentItem.context,
                    hasForm: true,
                    pageReference: context || this.pConn$.getPageReference()
                  };

                  if (!this.bSubscribed) {
                    this.bSubscribed = true;

                    var _this$PCore$$getConst7 = this.PCore$.getConstants(),
                        CONTAINER_TYPE = _this$PCore$$getConst7.CONTAINER_TYPE,
                        PUB_SUB_EVENTS = _this$PCore$$getConst7.PUB_SUB_EVENTS;

                    this.routingInfoRef["current"] = routingInfo;
                    this.PCore$.getPubSubUtils().subscribe(PUB_SUB_EVENTS.EVENT_SHOW_CANCEL_ALERT, function (payload) {
                      _this65.showAlert(payload);
                    }, PUB_SUB_EVENTS.EVENT_SHOW_CANCEL_ALERT, this.routingInfoRef);
                  } //let configObject = this.PCore$.createPConnect(config);


                  var configObject = this.getConfigObject(currentItem, this.pConn$); // THIS is where the ViewContainer creates a View
                  //    The config has meta.config.type = "view"

                  var newComp = configObject.getPConnect();
                  var newCompName = newComp.getComponentName();
                  var caseInfo = newComp && newComp.getDataObject() && newComp.getDataObject().caseInfo ? newComp.getDataObject().caseInfo : null; // The metadata for pyDetails changed such that the "template": "CaseView"
                  //  is no longer a child of the created View but is in the created View's
                  //  config. So, we DON'T want to replace this.pConn$ since the created
                  //  component is a View (and not a ViewContainer). We now look for the
                  //  "template" type directly in the created component (newComp) and NOT
                  //  as a child of the newly created component.
                  //console.log(`---> ModalViewContainer created new ${newCompName}`);
                  // Use the newly created component (View) info but DO NOT replace
                  //  this ModalViewContainer's pConn$, etc.
                  //  Note that we're now using the newly created View's PConnect in the
                  //  ViewContainer HTML template to guide what's rendered similar to what
                  //  the Nebula/Constellation return of React.Fragment does
                  // right now need to check caseInfo for changes, to trigger redraw, not getting 
                  // changes from angularPconnect except for first draw

                  if (newComp && caseInfo && this.compareCaseInfoIsDifferent(caseInfo)) {
                    this.psService.sendMessage(false);
                    this.ngZone.run(function () {
                      _this65.createdViewPConn$ = newComp;
                      var newConfigProps = newComp.getConfigProps();
                      _this65.templateName$ = 'template' in newConfigProps ? newConfigProps["template"] : "";
                      var actionName = latestItem.actionName,
                          isMinimizable = latestItem.isMinimizable;
                      var caseInfo = newComp.getCaseInfo();
                      var caseName = caseInfo.getName();
                      var ID = caseInfo.getID();
                      _this65.title$ = actionName || "New ".concat(caseName, " (").concat(ID, ")"); // // update children with new view's children

                      _this65.arChildren$ = newComp.getChildren();
                      _this65.bShowModal$ = true; // for when non modal

                      _this65.modalVisibleChange.emit(_this65.bShowModal$); // save off itemKey to be used for finishAssignment, etc.


                      _this65.itemKey$ = key; // cause a change for assignment

                      _this65.updateToken$ = new Date().getTime();
                    });
                  }
                }
              } else {
                this.hideModal();
              }
            }
          }
        }, {
          key: "hideModal",
          value: function hideModal() {
            var _this66 = this;

            if (this.bShowModal$) {// other code in Nebula/Constellation not needed currently, but if so later, 
              // should put here
            }

            this.ngZone.run(function () {
              _this66.bShowModal$ = false; // for when non modal

              _this66.modalVisibleChange.emit(_this66.bShowModal$);

              _this66.oCaseInfo = {};
            });
          }
        }, {
          key: "getConfigObject",
          value: function getConfigObject(item, pConnect) {
            if (item) {
              var context = item.context,
                  view = item.view;
              var config = {
                meta: view,
                options: {
                  context: context,
                  pageReference: view.config.context || pConnect.getPageReference(),
                  hasForm: true,
                  containerName: (pConnect === null || pConnect === void 0 ? void 0 : pConnect.getContainerName()) || this.PCore$.getConstants().MODAL
                }
              };
              return this.PCore$.createPConnect(config);
            }

            return null;
          }
        }, {
          key: "onAlertState",
          value: function onAlertState(bData) {
            this.bAlertState = bData;
            this.bShowCancelAlert$ = false;

            if (this.bAlertState == false) {
              this.hideModal();
            }
          }
        }, {
          key: "showAlert",
          value: function showAlert(payload) {
            var _this67 = this;

            var _this$getKeyAndLatest2 = this.getKeyAndLatestItem(this.routingInfoRef["current"]),
                latestItem = _this$getKeyAndLatest2.latestItem;

            var isModalAction = payload.isModalAction;
            /*
              If we are in create stage full page mode, created a new case and trying to click on cancel button
              it will show two alert dialogs which is not expected. Hence isModalAction flag to avoid that.
            */

            if (latestItem && isModalAction) {
              var configObject = this.getConfigObject(latestItem, this.pConn$);
              this.ngZone.run(function () {
                _this67.cancelPConn$ = configObject.getPConnect();
                _this67.bShowCancelAlert$ = true;
              });
            }
          }
        }, {
          key: "hasContainerItems",
          value: function hasContainerItems(routingInfo) {
            if (routingInfo) {
              var accessedOrder = routingInfo.accessedOrder,
                  items = routingInfo.items;
              return accessedOrder && accessedOrder.length > 0 && items;
            }

            return false;
          }
        }, {
          key: "getKeyAndLatestItem",
          value: function getKeyAndLatestItem(routinginfo) {
            if (this.hasContainerItems(routinginfo)) {
              var accessedOrder = routinginfo.accessedOrder,
                  items = routinginfo.items;
              var key = accessedOrder[accessedOrder.length - 1];
              var latestItem = items[key];
              return {
                key: key,
                latestItem: latestItem
              };
            }

            return {};
          }
        }, {
          key: "compareCaseInfoIsDifferent",
          value: function compareCaseInfoIsDifferent(oCurrentCaseInfo) {
            var bRet = false; // fast-deep-equal version

            if (fast_deep_equal__WEBPACK_IMPORTED_MODULE_7__ !== undefined) {
              bRet = !fast_deep_equal__WEBPACK_IMPORTED_MODULE_7__(this.oCaseInfo, oCurrentCaseInfo);
            } else {
              var sCurrnentCaseInfo = JSON.stringify(oCurrentCaseInfo);
              var sOldCaseInfo = JSON.stringify(this.oCaseInfo); // stringify compare version

              if (sCurrnentCaseInfo != sOldCaseInfo) {
                bRet = true;
              }
            } // if different, save off new case info


            if (bRet) {
              this.oCaseInfo = JSON.parse(JSON.stringify(oCurrentCaseInfo));
            }

            return bRet;
          }
        }]);

        return ModalViewContainerComponent;
      }();

