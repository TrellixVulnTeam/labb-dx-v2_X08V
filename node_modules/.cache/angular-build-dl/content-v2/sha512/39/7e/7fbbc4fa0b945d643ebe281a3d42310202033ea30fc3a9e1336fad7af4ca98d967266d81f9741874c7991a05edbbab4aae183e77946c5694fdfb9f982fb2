          key: "findCurrentIndicies",
          value: function findCurrentIndicies(arStepperSteps, arIndicies, depth) {
            var _this32 = this;

            var count = 0;
            arStepperSteps.forEach(function (step) {
              if (step.visited_status == "current") {
                arIndicies[depth] = count; // add in

                step["step_status"] = "";
              } else if (step.visited_status == "success") {
                count++;
                step.step_status = "completed";
              } else {
                count++;
                step.step_status = "";
              }

              if (step.steps) {
                arIndicies = _this32.findCurrentIndicies(step.steps, arIndicies, depth + 1);
              }
            });
